class Solution {
    public int subarrayXor(int[] arr) {
        // code here
        int n = arr.length;
        int res = 0;
        for (int i = 0; i < n; ++i) {
            long left = i + 1L;
            long right = n - i;
            if (((left & 1L) == 1L) && ((right & 1L) == 1L)) {
                res ^= arr[i];
            }
        }
        return res;
    }
}
