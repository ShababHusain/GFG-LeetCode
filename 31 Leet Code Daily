class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        int n = nums.length-2;
        int[] freq = new int[n];
        List<Integer> result = new ArrayList<>();

        for(int num:nums){
            freq[num]++;
            if(freq[num]==2){
                result.add(num);
            }
        }
        return new int[]{result.get(0), result.get(1)};
        
    }
}
