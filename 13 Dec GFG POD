class Solution {
    public void swapDiagonal(int[][] mat) {
        if (mat == null || mat.length == 0) return;
        int n = mat.length;
        for (int i = 0; i < n; ++i) {
            int j = n - 1 - i;
            int tmp = mat[i][i];
            mat[i][i] = mat[i][j];
            mat[i][j] = tmp;
        }
    }
}
