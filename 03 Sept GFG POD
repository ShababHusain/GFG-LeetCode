class Solution {
    public Node reverse(Node head) {
        Node curr = head;
        Node temp = null;

        while (curr != null) {
            // Swap next and prev
            temp = curr.prev;
            curr.prev = curr.next;
            curr.next = temp;

            // Move to next node (which is prev now)
            head = curr;
            curr = curr.prev;
        }

        return head;
    }
}
