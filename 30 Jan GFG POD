class Solution {
    public void rearrangeQueue(Queue<Integer> q) {
        int n = q.size();
        int half = n / 2;

        // Step 1: Store first half in a stack (or another queue)
        Queue<Integer> firstHalf = new LinkedList<>();
        for (int i = 0; i < half; i++) {
            firstHalf.add(q.poll());
        }

        // Step 2: Interleave elements from firstHalf and remaining queue
        while (!firstHalf.isEmpty()) {
            q.add(firstHalf.poll());  // element from first half
            q.add(q.poll());          // element from second half
        }
    }

    // Utility method to print queue
    public static void printQueue(Queue<Integer> q) {
        for (int num : q) {
            System.out.print(num + " ");
        }
        System.out.println();
    }

    
}
