class Spreadsheet {
    private Map<String, Integer> cells;

    public Spreadsheet(int rows) {
        cells = new HashMap<>();
    }

    public void setCell(String cell, int value) {
        cells.put(cell, value);
    }

    public void resetCell(String cell) {
        cells.put(cell, 0);
    }

    public int getValue(String formula) {
        // Remove leading '=' and split by '+'
        String[] parts = formula.substring(1).split("\\+");
        return getOperandValue(parts[0]) + getOperandValue(parts[1]);
    }

    private int getOperandValue(String token) {
        // If token starts with a digit, it's a number
        if (Character.isDigit(token.charAt(0))) {
            return Integer.parseInt(token);
        }
        // Otherwise, it's a cell reference
        return cells.getOrDefault(token, 0);
    }
}


/**
 * Your Spreadsheet object will be instantiated and called as such:
 * Spreadsheet obj = new Spreadsheet(rows);
 * obj.setCell(cell,value);
 * obj.resetCell(cell);
 * int param_3 = obj.getValue(formula);
 */
